#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/newProj/tx-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/newProj/tx-cli/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../seek-bzip@1.0.6/node_modules/seek-bzip/bin/seek-bzip-table" "$@"
else
  exec node  "$basedir/../../../../../seek-bzip@1.0.6/node_modules/seek-bzip/bin/seek-bzip-table" "$@"
fi
